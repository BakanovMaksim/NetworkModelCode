<Window x:Class="NetworkModelCode.DesktopWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetworkModelCode.DesktopWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterOwner" WindowState="Maximized">

    <DockPanel x:Name="mainDockPanel">
        <Menu x:Name="applicationMenu" DockPanel.Dock="Top" HorizontalAlignment="Center">
            <MenuItem x:Name="projectMenuItem" Header="Проект">
                <MenuItem Header="Новый проект" Click="MenuItem_Click"></MenuItem>
                <MenuItem x:Name="existingProjectMenuItem" Header="Последний проект" Click="existingProjectMenuItem_Click" ></MenuItem>
            </MenuItem>
            <MenuItem x:Name="goBackMenuItem" Header="Назад" Click="goBackMenuItem_Click"/>
        </Menu>
        <Frame x:Name="mainFrame" DockPanel.Dock="Bottom">
            
        </Frame>
    </DockPanel>
</Window>
