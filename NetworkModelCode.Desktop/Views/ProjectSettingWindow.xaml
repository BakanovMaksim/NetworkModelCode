<Window x:Class="NetworkModelCode.Desktop.Views.ProjectSettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetworkModelCode.Desktop.Views"
        mc:Ignorable="d"
        Title="Настройка проекта" Height="450" Width="800">

    <StackPanel VerticalAlignment="Center">
        <TextBox HorizontalContentAlignment="Center" Text="{Binding Title}"/>
        <TextBox HorizontalContentAlignment="Center" Text="{Binding WorkCount}"/>
        <Button x:Name="saveSettingButton" Content="Сохранить" Click="saveSettingButton_Click"/>
    </StackPanel>
</Window>
